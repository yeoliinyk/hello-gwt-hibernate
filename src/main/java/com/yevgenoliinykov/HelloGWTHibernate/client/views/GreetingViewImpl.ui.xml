<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.greetingContainer{
		margin: 20px;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.greetingContainer}">
		<table>
			<tr>
				<td>
					<g:Label ui:field="lblGreeting" text="" />
				</td>
			</tr>
			<tr>
				<td>
					<g:Anchor ui:field="ancLogOut" href="javascript:;" text=""></g:Anchor>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 